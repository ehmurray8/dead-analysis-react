{
  "name": "server",
  "version": "1.0.0",
  "description": "Music analysis server.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./build && mkdir ./build",
    "build-server": "babel ./server.js --out-dir ./build -s",
